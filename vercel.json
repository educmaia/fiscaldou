{
  "functions": {
    "api/fiscaldou.py": {
      "runtime": "python3.9"
    }
  },
  "crons": [
    {
      "path": "/api/cron/daily",
      "schedule": "0 11 * * *"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/fiscaldou.py"
    }
  ],
  "build": {
    "env": {
      "PYTHONDONTWRITEBYTECODE": "1",
      "PIP_DISABLE_PIP_VERSION_CHECK": "1"
    }
  }
}
